{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "useUnknownInCatchVariables": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/context/UserPreferencesProvider.tsx", "src/utils/useLocalFireStoreDocument.js", "src/pages/_app.js", "src/components/Shared/NavigationLink.jsx", "src/utils/withOpacity.js"  ],
  "exclude": [
    "node_modules"
  ]
}
